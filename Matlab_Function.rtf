\fs20 \cf0 dydt = zeros(8,1);
\fs24 \
\pard\pardeftab720\partightenfactor0

\fs20 \cf3 %%%%%%%%%%%%%%%%%%%%
\fs24 \cf0 \
\pard\pardeftab720\partightenfactor0

\fs20 \cf0 L = 100;
\fs24 \

\fs20 Mu_a = 0.085;
\fs24 \

