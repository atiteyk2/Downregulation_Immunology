\fs20     dydt(2) = r(1)*IL4*y(1) - r(2)*y(2) - Ab_4*y(2); \cf3 % EQUATION (3)
\fs24 \cf0 \

\fs20     
\fs24 \

\fs20     dydt(3) = No_2(i) - r(3)*IL2*y(3) + r(4)*y(4) - Au_2*y(3) - syn*y(2)*y(3); \cf3 % EQUATION (4)
\fs24 \cf0 \

\fs20     dydt(4) = r(3)*IL2*y(3) - r(4)*y(4) - Ab_2*y(4);  \cf3 %EQUATION (5)
\fs24 \cf0 \

\fs20     
\fs24 \

\fs20     dydt(5) =  Rate_prod*y(6) + Gs*y(5)*y(4)/(y(4) + Tr_IL2b)- Mu_death*y(5); \cf3 % EQUATION (8) % # Rate_prod*y(2) + #- r(5)*AntiG(i)*y(5);
\fs24 \cf0 \

\fs20     dydt(6) = r(6)*AntiG(i) - AntiG_die*y(6);  \cf3 % EQUATION (9)
\fs24 \cf0 \

\fs20     
\fs24 \

\fs20     dydt(7) = r(7)*y(5)*y(2)/(y(2) + Tr_IL2b) + G2*y(7)*y(4)/(y(4) + s_IL4b) - (Mu_Bdeath + Mu_Bdiff)*y(7); \cf3 % EQUATION (6) % # Rate_prod*y(2) + #- r(5)*AntiG(i)*y(5);
\fs24 \cf0 \

\fs20     dydt(8) = Gp*y(7); \cf3 % EQUATION (7)
\fs24 \cf0 \
