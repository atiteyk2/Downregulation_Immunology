\fs20 Mu_Bdeath = 0.01; \cf3 %B cell death rate
\fs24 \cf0 \

\fs20 Mu_Bdiff = 0.16e4; \cf3 %B cell diff rate
\fs24 \cf0 \

\fs20 s_IL4b = 18e4; \cf3 % Threshold number of IL4
\fs24 \cf0 \

\fs20 G2 = 0.0035; \cf3 % B cell simulation rate by IL2
