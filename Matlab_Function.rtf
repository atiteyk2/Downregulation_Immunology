\fs24 \

\fs20 Th0 = 805;
\fs24 \

\fs20 Mu_death = 0.01; \cf3 % T cell death rate
\fs24 \cf0 \
\pard\pardeftab720\partightenfactor0

\fs20 \cf3  
\fs24 \cf0 \
\pard\pardeftab720\partightenfactor0

\fs20 \cf0 Rate_prod = Th0*Mu_death;
\fs24 \

\fs20 AntiG = lognrnd(Mu_a,Sig,1,L);
\fs24 \

\fs20 AntiG_die = 3.97e-2; \cf3 % rate of antigen dearth
\fs24 \cf0 \
\pard\pardeftab720\partightenfactor0

\fs20 \cf3 %%%%%%%%%%%%%%%%%%%%
\fs24 \cf0 \

\fs20 \cf3 %%
\fs24 \cf0 \
\pard\pardeftab720\partightenfactor0

\fs20 \cf0 Ab_2 = 5.62e-4; \cf3 % Internalization rate of bound receptors
\fs24 \cf0 \

\fs20 Au_2 = 1.4e-4; \cf3 % Internalization  rates of unbound receptors
\fs24 \cf0 \

\fs20 Ab_4 = 5.27e-4; \cf3 % Internalization rate of bound receptors
\fs24 \cf0 \

\fs20 Au_4 = 0.35e-4; \cf3 % Internalization  rates of unbound receptors
\fs24 \cf0 \
\pard\pardeftab720\partightenfactor0

\fs20 \cf3 %%
\fs24 \cf0 \

\fs20 \cf3 % L = 100;
\fs24 \cf0 \
\pard\pardeftab720\partightenfactor0

\fs20 \cf0 IL2 = 1.5e-11; \cf3 % # 3e-11; % # 1.5e-11;
\fs24 \cf0 \

\fs20 IL4 = 1.5e-10; \cf3 %3.5e-11; % Mole
\fs24 \cf0 \

\fs20 mu_2 = log(1698);      
\fs24 \

\fs20 sigma_2 = 1; 
\fs24 \

\fs20 mu_4 = log(7080);
\fs24 \

\fs20 sigma_4 = 1;
\fs24 \
\pard\pardeftab720\partightenfactor0

\fs20 \cf3 %%
\fs24 \cf0 \
\pard\pardeftab720\partightenfactor0

\fs20 \cf0 No_2 = lognrnd(mu_2,sigma_2,1,L); \cf3 % Number of receptors on the cell surface 
\fs24 \cf0 \

\fs20 No_4 = lognrnd(mu_4,sigma_4,1,L);
\fs24 \

\fs20  
\fs24 \

\fs20 syn = 8.8e-8; \cf3 % synergy
\fs24 \cf0 \

\fs20 Gs = 0.009;
\fs24 \

\fs20  
\fs24 \

\fs20 Tr_IL2b = 2e4; \cf3 % Threshold number of IL4
